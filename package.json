{
	"name": "lowclass",
	"version": "4.9.0",
	"description": "JavaScript/TypeScript class inheritance tools.",
	"type": "module",
	"COMMENT-main-types-fields": "The 'main' and 'types' fields are relative to dist/ after building.",
	"main": "index.js",
	"types": "index.d.ts",
	"scripts": {
		"clean": "builder run clean",
		"build": "builder run build",
		"dev": "builder run dev",
		"test": "builder run npm:test",
		"test:debug": "builder run npm:test-debug",
		"tsc": "builder run tsc",
		"tsc:watch": "builder run tsc:watch",
		"typecheck": "builder run typecheck",
		"typecheck:watch": "builder run typecheck:watch",
		"prettier": "builder run prettier",
		"release:patch": "builder run release:patch",
		"release:minor": "builder run release:minor",
		"release:major": "builder run release:major",
		"version": "builder run npm:version",
		"postversion": "builder run npm:postversion",
		"serve": "browser-sync start --server ./ --files dist --port 12345"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/trusktr/lowclass.git"
	},
	"author": "Joe Pea <joe@trusktr.io>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/trusktr/lowclass/issues"
	},
	"homepage": "http://github.com/trusktr/lowclass#readme",
	"dependencies": {
		"@babel/runtime": "7.0.0-beta.46"
	},
	"devDependencies": {
		"browser-sync": "^2.26.7",
		"builder": "4.0.0",
		"builder-js-package": "trusktr/builder-js-package#34c9e8ee01319f73f76f9facb410d7e7aff84db4",
		"prettier": "^1.19.1"
	},
	"keywords": [
		"classes",
		"es5 classes",
		"classical inheritance",
		"classical",
		"prototype",
		"prototypical inheritance",
		"prototypes",
		"prototype classes",
		"private",
		"protected",
		"access specifiers",
		"access modifiers"
	]
}
