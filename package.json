{
  "name": "lowclass",
  "version": "0.0.1",
  "description": "Low-complexity class inheritance.",
  "format": "es5",
  "main": "index.js",
  "browser": "global.js",
  "global": "global.js",
  "license": "MIT",
  "homepage": "http://github.com/trusktr/clath",
  "scripts": {
    "test": "echo 'TODO: Make tests.' && exit 1",
    "clean": "mkdir save && mv webpack.config.js save && rm -rf ./*.js ./*.js.map && mv save/* . && rmdir save",
    "prepublish": "npm run build-cjs",
    "build-umd": "babel src --source-maps --out-dir . --modules umd",
    "build-cjs": "babel src --source-maps --out-dir . --modules common",
    "build-amd": "babel src --source-maps --out-dir . --modules amd",
    "build-global": "webpack --progress --colors --output-library-target umd src/index.js global.js --output-library lowclass"
  },
  "dependencies": {
    "regexr": "^1.1.1"
  },
  "devDependencies": {
    "babel": "^5.6.6",
    "babel-loader": "^5.3.2",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "glslify-loader": "^1.0.2",
    "raw-loader": "^0.5.1",
    "webpack": "^1.12.1"
  },
  "jspm": {
    "registry": "jspm",
    "directories": {
      "lib": "src"
    }
  },
  "bower": {
    "TODO": "TODO"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infamous/motor.git"
  },
  "bugs": {
    "url": "https://github.com/infamous/motor/issues"
  },
  "keywords": [
    "classes",
    "es5 classes",
    "classical inheritance",
    "classical",
    "prototype",
    "prototypical inheritance",
    "prototypes",
    "prototype classes"
  ],
  "author": "joe <joe@trusktr.io>"
}
